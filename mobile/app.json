{
  "expo": {
    "name": "Reo",
    "slug": "reo-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "reo",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reo.app"
    },
    "android": {
      "package": "com.reo.app"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}